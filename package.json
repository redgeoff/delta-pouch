{
  "name": "delta-pouch",
  "version": "0.1.7",
  "description": "Conflict-free collaborative editing for PouchDB",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/redgeoff/delta-pouch.git"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "plugin",
    "delta",
    "partial",
    "updates",
    "couch",
    "couchdb"
  ],
  "author": "Geoffrey Cox",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/redgeoff/delta-pouch/issues"
  },
  "scripts": {
    "test-node": "TEST_DB=testdb,http://localhost:5984/testdb istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "test-browser": "./bin/test-browser.js",
    "jshint": "jshint -c .jshintrc *.js test/test.js examples/websites/*.js",
    "test": "npm run jshint && ./bin/run-test.sh",
    "build": "mkdir -p dist && browserify index.js -o dist/pouchdb.delta-pouch.js && npm run min",
    "min": "uglifyjs dist/pouchdb.delta-pouch.js -mc > dist/pouchdb.delta-pouch.min.js",
    "dev": "browserify test/test.js > test/test-bundle.js && npm run dev-server",
    "dev-server": "./bin/dev-server.js",
    "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
  },
  "dependencies": {
    "argsarray": "0.0.1",
    "es3ify": "^0.2.2",
    "inherits": "^2.0.3",
    "lie": "^3.1.1",
    "phantomjs-prebuilt": "^2.1.14"
  },
  "devDependencies": {
    "bluebird": "^3.5.0",
    "browserify": "^14.3.0",
    "chai": "^4.0.0",
    "chai-as-promised": "^7.0.0",
    "events": "^1.1.1",
    "http-server": "^0.10.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.4.2",
    "pouchdb": "^6.2.0",
    "pouchdb-adapter-memory": "^6.2.0",
    "request": "^2.81.0",
    "sauce-connect-launcher": "^1.2.2",
    "uglify-js": "^3.0.11",
    "watchify": "^3.9.0",
    "wd": "^1.2.0"
  },
  "browser": {
    "crypto": false
  },
  "browserify": {
    "transform": [
      "es3ify"
    ]
  }
}
